<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CourseEntryActivity">

  <TextView
      android:id="@+id/textView5"
      android:layout_width="201dp"
      android:layout_height="27dp"
      android:text="Enter Course Number"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.596" />

  <EditText
      android:id="@+id/plain_text_input4"
      android:layout_width="178dp"
      android:layout_height="41dp"
      android:hint="Course Number"
      android:inputType="text"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.506"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView5"
      app:layout_constraintVertical_bias="0.0" />

  <Button
      android:id="@+id/course_entry_done_button"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Finish"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.801"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.855" />

  <Button
      android:id="@+id/enter_course_button"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Enter"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.201"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.855" />

  <TextView
      android:id="@+id/textView"
      android:layout_width="94dp"
      android:layout_height="29dp"
      android:text="Enter Year"
      android:textSize="20sp"
      app:layout_constraintBottom_toTopOf="@+id/plain_text_input5"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.52"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="1.0" />

  <EditText
      android:id="@+id/plain_text_input5"
      android:layout_width="178dp"
      android:layout_height="41dp"
      android:layout_marginBottom="40dp"
      android:hint="Year"
      android:inputType="text"
      app:layout_constraintBottom_toTopOf="@+id/textView3"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.51"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.883" />

  <TextView
      android:id="@+id/textView4"
      android:layout_width="125dp"
      android:layout_height="26dp"
      android:text="Enter Subject"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.51"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.418" />

  <EditText
      android:id="@+id/plain_text_input"
      android:layout_width="178dp"
      android:layout_height="41dp"
      android:hint="Subject"
      android:inputType="text"
      app:layout_constraintBottom_toTopOf="@+id/textView5"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.506"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView4"
      app:layout_constraintVertical_bias="0.0" />

  <TextView
      android:id="@+id/textView3"
      android:layout_width="131dp"
      android:layout_height="30dp"
      android:text="Select Quarter"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.521"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.262" />

  <Spinner
      android:id="@+id/quarter_selector"
      android:layout_width="179dp"
      android:layout_height="36dp"
      app:layout_constraintBottom_toTopOf="@+id/textView4"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.508"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView3"
      app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>